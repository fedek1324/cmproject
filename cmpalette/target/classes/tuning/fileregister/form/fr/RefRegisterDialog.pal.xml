<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
    <form schema-ref="RefRegisterModif" projection="default" name="${ui.form.FrDialog.name:Выберите дело}" id="RefRegisterDialog">
        <properties>
            <hide condition-union="AND">
                <negative-condition-ref ref="dialog" />
            </hide>
        </properties>
        <component>
            <field attribute-ref="sdObjectInfo" tid="sdObjectInfo" name="${ui.form.RefRegisterDialog.field.vacancyInfo.name:Выбор дела:}">
                <appearance importance="normal" widget="grid"/>
                <directories>
                    <directory-category-custom-coll attribute-ref="parentUnit" ref="fr-vw-cmj-nd" />
                </directories>
            </field>
            <button name="${ui.form.DialogNdpSelectInNdoChangeForm.button.OK.name:OK}" tid="submit">
                <action>
                    <system name="submit"/>
                </action>
            </button>
        </component>
        <appearance>
            <dialog-mode height="50" unit="percent" width="50" />
        </appearance>
    </form>
</ui>
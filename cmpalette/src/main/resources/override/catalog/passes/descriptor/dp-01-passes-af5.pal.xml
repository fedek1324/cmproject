<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
    <custom-descriptor ident="Passes" view="Passes_passesSQLP" id="dp-01-passes-af5" name="Пропуска" style="default">
        <builder name="ru.intertrust.cm_sochi.srv.uicollections.UiBuilder"/>
        <table>
            <columns>
                <!-- reason,
                date,
                forWho,
                toWho,
                carNumber,
                carModel -->
                <column-ref ref="self"/>
                <column-ref ref="reason" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="date" filter="true" sort-desc="true" sort-asc="true"/>
                <column-ref ref="forWho" filter="true" sort-desc="true" sort-asc="true"/>
                <column-ref ref="toWho" filter="true" sort-desc="true" sort-asc="true"/>
                <column-ref ref="carNumber" filter="true" sort-desc="true" sort-asc="true"/>
                <column-ref ref="carModel" filter="true" sort-desc="true" sort-asc="true"/>
                <column-ref ref="subject"/>
            </columns>
        </table>
    </custom-descriptor>
    <column name="reason" extends="string" subject="${reason:Цель}"/>
    <column name="date" extends="date" subject="${date:Дата получения пропуска}"/>
    <column name="forWho" extends="string" subject="${forWho:Для кого}"/>
    <column name="toWho" extends="string" subject="${toWho:К кому}"/>
    <column name="carNumber" extends="string" subject="${carNumber:Номер машины}"/>
    <column name="carModel" extends="string" subject="${carModel:Модель машины}"/>
</catalog>
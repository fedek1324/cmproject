    <string name="reason" required="true">
      <storage-field name="reason"/>
    </string>
    <datetime name="giveDateTime">
      <storage-field name="giveDateTime"/>
    </datetime>
    <actor array="false" name="forWho" />
    <actor array="false" name="toWho" />

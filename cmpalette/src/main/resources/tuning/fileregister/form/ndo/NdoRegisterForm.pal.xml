<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="NdoRegister" id="NdoRegisterForm" projection="default" showtabs="true">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="edit"/>
        <negative-condition-ref ref="read"/>
      </hide>
    </properties>
    <appearance widget="tabs"/>
    <component>
      <section id="RootMenu">
        <component>
          <section name="${ui.form.NdoRegisterForm.section.RootMenu.section.CREATE.name:Создать}" id="CREATE">
            <component>
              <button name="${ui.form.NdoRegisterForm.section.RootMenu.section.CREATE.button.CreateNewNDO.name:Новую Сводную НД}" alias="create-new-ndo">
                <action>
                  <system name="http://intertrust.ru/cmj/relations#create-new-object"/>
                </action>
              </button>
              <button name="${ui.form.NdoRegisterForm.section.RootMenu.section.CREATE.button.CreateNdoChange.name:Создать изменения к НДО}" alias="create-ndochange">
                <properties>
                  <hide condition-union="AND">
                    <negative-condition-ref ref="read"/>
                    <negative-condition-ref ref="canCreateNoChange"/>
                  </hide>
                </properties>
                <action>
                  <custom>
                    <operation>
                      <handler>
                        <script lang="JavaBean">CreateNewNdoChangeForNdo</script>
                      </handler>
                    </operation>
                  </custom>
                </action>
              </button>
            </component>
            <appearance>
              <action position="8.0">
                <system name="CREATE"/>
              </action>
            </appearance>
          </section>
          <button name="${ui.form.NdoRegisterForm.section.RootMenu.button.AddPartitions.name:Добавить раздел}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="canAddOrCreateChapterNdo" />
              </hide>
            </properties>
            <action position="20.0">
              <custom>
                <operation>
                  <handler>
                    <script lang="JavaBean">AddExistsNdpIntoNdo</script>
                  </handler>
                </operation>
              </custom>
            </action>
          </button>
          <button name="${ui.form.NdoRegisterForm.section.RootMenu.button.ExcludePartitions.name:Исключить раздел}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="canExcludeUnitRegisters" />
              </hide>
            </properties>
            <action position="20.1">
              <custom>
                <operation>
                  <handler>
                    <script lang="JavaBean">ExcludeNdpFromNdo</script>
                  </handler>
                </operation>
              </custom>
            </action>
          </button>
          <button name="${ui.form.NdoRegisterForm.section.RootMenu.button.ReturnForRevisionNdo.name:Вернуть на доработку}" tid="ReturnForRevisionNdo">
              <properties>
                  <hide condition-union="AND">
                      <negative-condition-ref ref="canSeeButtonReturnForRevisionNdo"/>
                  </hide>
              </properties>
              <action position="20.2">
                  <custom>
                      <operation>
                          <handler>
                              <script lang="JavaBean">ReturnForRevisionNdo</script>
                          </handler>
                      </operation>
                  </custom>
              </action>
          </button>
          <button name="${ui.form.NdoRegisterForm.section.RootMenu.button.ComposeFinalNote.name:Составить итоговую запись}"
                  tid="ComposeFinalNote">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="canSeeButtonComposeFinalNoteNdo"/>
              </hide>
            </properties>
            <appearance align="left">
              <label-style width="20">
                <style id="tunUnderline" />
                <style id="tunClickableRed" />
              </label-style>
            </appearance>
            <action>
              <custom>
                <operation>
                  <handler>
                    <script lang="JavaBean">ComposeFinalNoteForNdo</script>
                  </handler>
                </operation>
              </custom>
            </action>
          </button>
          <button name="${ui.form.NdoRegisterForm.section.RootMenu.button.Verify.name:Заверить}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="canSeeButtonVerifyNdo"/>
              </hide>
            </properties>
            <appearance align="left">
              <label-style width="20">
                <style id="tunUnderline" />
                <style id="tunClickableRed" />
              </label-style>
            </appearance>
            <action>
              <custom>
                <operation>
                  <handler>
                    <script lang="JavaBean">VerifyFrOrgRegister</script>
                  </handler>
                </operation>
              </custom>
            </action>
          </button>
          <section id="SEND" name="${ui.form.NdoRegisterForm.section.RootMenu.section.SEND.name:Отправить}">
            <component>
              <button attribute-ref="$canSendToApproving" name="${ui.form.NdoRegisterForm.button.canSendToApproving.name:На согласование}">
                <properties>
                  <hide condition-union="AND">
                    <negative-condition-ref ref="CanSendToApproving"/>
                  </hide>
                </properties>
                <hint-pop-up>${ui.form.NdoRegisterForm.button.canSendToApproving.hintPopUp:На согласование}</hint-pop-up>
                <action>
                  <system name="http://intertrust.ru/cmj/relations#approving.start"/>
                </action>
              </button>
            </component>
          </section>
          <section name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.name:Другое}" id="OTHER">
            <component>
              <button name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.button.Readers.name:Читатели}" alias="ndo-readers" tid="ndo-readers">
                <properties>
                  <hide condition-union="AND">
                    <negative-condition-ref ref="read"/>
                  </hide>
                </properties>
                <action>
                  <custom>
                    <operation>
                      <handler>
                        <script lang="JavaBean">IndicateReadersOperation</script>
                      </handler>
                    </operation>
                  </custom>
                </action>
              </button>
              <button name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.button.printForm.name:Скрыть системную кнопку Печатная форма}" alias="hiddenPrintForm" tid="def-printform">
                <properties>
                  <hide condition-union="AND">
                    <condition>true</condition>
                  </hide>
                </properties>
                <action>
                  <system name="PRINTED_FORM"/>
                </action>
              </button>
              <section name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.section.PRINT_REPORT.name:Печатная форма}" id="PRINT_REPORT">
                <component>
                  <button alias="org-register-with-one-approval-mark" tid="ndoWith1ApprovalMark"
                          name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.section.PRINT_REPORT.button.ndoWith1ApprMark.name:Номенклатура дел организации с одной отметкой согласования (ЭК)}">
                    <properties>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="read"/>
                      </hide>
                    </properties>
                    <action>
                      <custom>
                        <operation>
                          <handler>
                            <script lang="JavaBean">PrintformNdo</script>
                          </handler>
                        </operation>
                      </custom>
                    </action>
                  </button>
                  <button alias="org-register-with-two-approval-marks" tid="ndoWith2ApprovalMarks"
                          name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.section.PRINT_REPORT.button.ndoWith2ApprovalMarks.name:Номенклатура дел организации с двумя отметками согласования (ЭК и ЭПК)}">
                    <properties>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="read"/>
                      </hide>
                    </properties>
                    <action>
                      <custom>
                        <operation>
                          <handler>
                            <script lang="JavaBean">PrintformNdo</script>
                          </handler>
                        </operation>
                      </custom>
                    </action>
                  </button>
                  <button alias="extract-from-ndo-main" tid="extractFromNdo"
                          name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.section.PRINT_REPORT.button.extractFromNdo.name:Выписка из сводной Номенклатуры дел}">
                    <properties>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="read"/>
                      </hide>
                    </properties>
                    <action>
                      <custom>
                        <operation>
                          <handler>
                            <script lang="JavaBean">PrintformNdo</script>
                          </handler>
                        </operation>
                      </custom>
                    </action>
                  </button>
                  <button alias="final-record-to-ndo" tid="finalRecordToNdo"
                          name="${ui.form.NdoRegisterForm.section.RootMenu.section.OTHER.section.PRINT_REPORT.button.finalRecordToNdo.name:Итоговая запись к сводной номенклатуре дел}">
                    <properties>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="read"/>
                      </hide>
                    </properties>
                    <action>
                      <custom>
                        <operation>
                          <handler>
                            <script lang="JavaBean">PrintformNdo</script>
                          </handler>
                        </operation>
                      </custom>
                    </action>
                  </button>
                </component>
              </section>
            </component>
            <appearance>
              <action position="25">
                <system name="OTHER"/>
              </action>
            </appearance>
          </section>
        </component>
      </section>
      <section id="Header">
        <properties>
          <read-only>
            <condition-ref ref="exStatusIsEkApproved"/>
          </read-only>
        </properties>
        <component>
          <section id="Header_row1" group="true">
            <appearance field-layout="horizontal"/>
            <component>
              <section group="true" id="objectShow">
                <component>
                  <field attribute-ref="objectShow">
                    <properties>
                      <read-only condition-union="AND">
                        <condition>true</condition>
                      </read-only>
                    </properties>
                    <appearance align="left">
                      <label-style field-unit="symbol" field-width="18" />
                      <alt-appearancies>
                        <alt-appearance>
                          <appearance-ref ref="css_headerElementWithIcon" />
                          <enable condition-union="AND">
                            <condition>true</condition>
                          </enable>
                        </alt-appearance>
                      </alt-appearancies>
                    </appearance>
                  </field>
                </component>
                <appearance align="left">
                  <dialog-mode unit="percent" width="12" />
                </appearance>
              </section>
              <section group="true" id="organization">
                <component>
                  <field attribute-ref="organization">
                    <properties>
                      <require condition-union="AND">
                        <negative-condition-ref ref="edit" />
                        <condition>true</condition>
                      </require>
                      <read-only condition-union="AND">
                        <negative-condition-ref ref="TObjectIsNotNew" />
                      </read-only>
                    </properties>
                    <appearance align="left">
                      <label-style field-unit="symbol" field-width="18" />
                      <alt-appearancies>
                        <alt-appearance>
                          <appearance-ref ref="css_headerElementWithIcon" />
                          <enable condition-union="AND">
                            <condition>true</condition>
                          </enable>
                        </alt-appearance>
                      </alt-appearancies>
                    </appearance>
                    <directories>
                      <directory-ref ref="CURRENT_ORG_ONLY" />
                    </directories>
                  </field>
                </component>
                <appearance align="left">
                  <dialog-mode unit="percent" width="11" />
                </appearance>
              </section>
              <section group="true" id="orgDepartment">
                <component>
                  <field attribute-ref="orgDepartment">
                    <properties>
                      <read-only condition-union="AND">
                        <negative-condition-ref ref="TObjectIsNotNew" />
                      </read-only>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="TObjectIsNew" />
                        <negative-condition-ref ref="orgDepIsNotEmpty" />
                      </hide>
                    </properties>
                    <appearance align="left">
                      <label-style field-unit="symbol" field-width="18" />
                      <alt-appearancies>
                        <alt-appearance>
                          <appearance-ref ref="css_headerElementWithIcon" />
                          <enable condition-union="AND">
                            <condition>true</condition>
                          </enable>
                        </alt-appearance>
                      </alt-appearancies>
                    </appearance>
                    <directories relation="NdDepsHasNdo" limit-livesearch="sys-current" livesearch="true">
                      <directory-ref ref="NdDepsHasNdo" />
                    </directories>
                  </field>
                </component>
              </section>
              <button name="${ui.form.NdoRegister.label.readyForEkConsiderationNdo.name:Готова к вынесению на ЭК}" tid="readyForEkConsiderationNdo">
                <properties>
                  <hide>
                    <negative-condition-ref ref="NdoIsReadyForEkConsideration"/>
                  </hide>
                  <disabled>
                    <negative-condition-ref ref="canTouchNotReadyEK"/>
                  </disabled>
                </properties>
                <appearance>
                  <label-style>
                    <style id="tunBtnHeader" />
                    <style id="tunGreen" />
                  </label-style>
                </appearance>
                <action>
                  <custom>
                    <operation save-context="false">
                      <handler>
                        <script lang="JavaBean">ToggleReadyForEkConsiderationNdo</script>
                      </handler>
                    </operation>
                  </custom>
                </action>
              </button>
              <button name="${ui.form.NdoRegister.label.unreadyForEkConsiderationNdo.name:Не готова к вынесению на ЭК}" tid="unreadyForEkConsiderationNdo">
                <properties>
                  <hide>
                    <condition-ref ref="NdoIsReadyForEkConsideration"/>
                    <condition-ref ref="TObjectIsNew"/>
                  </hide>
                  <disabled>
                    <negative-condition-ref ref="canTouchReadyEK"/>
                  </disabled>
                </properties>
                <appearance>
                  <label-style>
                    <style id="tunBtnHeader" />
                    <style id="tunRed" />
                  </label-style>
                </appearance>
                <action>
                  <custom>
                    <operation save-context="false">
                      <handler>
                        <script lang="JavaBean">ToggleReadyForEkConsiderationNdo</script>
                      </handler>
                    </operation>
                  </custom>
                </action>
              </button>
              <button tid="readyEkApprovedRead" name="${ui.form.NdoRegister.label.readyEkApproved.name:Согласовано ЭК}">
                <properties>
                  <hide condition-union="AND">
                    <negative-condition-ref ref="NdoIsEkApproved" />
                  </hide>
                  <disabled condition-union="AND">
                    <negative-condition-ref ref="edit" />
                  </disabled>
                </properties>
                <appearance>
                  <label-style>
                    <style id="tunBtnHeader" />
                    <style id="tunGreen" />
                  </label-style>
                </appearance>
                <action>
                  <custom>
                    <operation save-context="false">
                      <handler>
                        <script lang="JavaBean">ToggleNdoIsEkApproved</script>
                      </handler>
                    </operation>
                  </custom>
                </action>
              </button>
              <button recalc="true" attribute-ref="setNotEkApprovedNdo" tid="unreadyEkApproved" name="${ui.form.NdoRegister.label.unreadyEkApproved.name:Не согласовано ЭК}">
                <properties>
                  <hide>
                    <condition-ref ref="NdoIsEkApproved" />
                    <condition-ref ref="TObjectIsNew" />
                  </hide>
                  <disabled condition-union="AND">
                    <negative-condition-ref ref="canTouchEKApproved" />
                  </disabled>
                </properties>
                <appearance>
                  <label-style>
                    <style id="tunBtnHeader" />
                    <style id="tunRed" />
                  </label-style>
                </appearance>
              </button>
              <field attribute-ref="statusGeneral">
                <properties>
                  <read-only condition-union="AND">
                    <condition>true</condition>
                  </read-only>
                </properties>
                <appearance align="right">
                  <label-style field-width="15" field-unit="symbol">
                    <style id="tunBold"/>
                  </label-style>
                  <field-style>
                    <value-style>
                      <style id="tunBold"/>
                    </value-style>
                  </field-style>
                </appearance>
                <directories limit-livesearch="no-restrictions">
                  <directory-ref ref="ndoStatus"/>
                </directories>
              </field>
             </component>
          </section>
          <section id="Header_row2" group="true">
            <appearance field-layout="horizontal"/>
            <component>
              <field attribute-ref="dateYearStr" name="${ui.form.NdoRegisterForm.label.dateYear.name:Год}">
                <properties>
                  <read-only>
                    <condition-ref ref="TObjectIsNotNew"/>
                  </read-only>
                </properties>
                <appearance importance="normal">
                </appearance>
                <directories limit-livesearch="no-restrictions">
                  <directory-ref ref="NDOYears"/>
                </directories>
              </field>
              <field attribute-ref="protocol" name="${ui.form.NdoRegisterForm.label.protocol.name:Протокол ЦЭК (ЭК) от:}">
                <properties>
                  <require condition-union="AND">
                    <condition>true</condition>
                  </require>
                  <hide>
                    <negative-condition-ref ref="NdoIsEkApproved"/>
                  </hide>
                </properties>
              </field>
              <field attribute-ref="additionalNum" name="${ui.form.NdoRegisterForm.label.additionalNum.name:№}">
                <properties>
                  <require condition-union="AND">
                    <condition>true</condition>
                  </require>
                  <hide>
                    <negative-condition-ref ref="NdoIsEkApproved"/>
                  </hide>
                </properties>
              </field>
              <field attribute-ref="apre">
                <properties>
                  <read-only>
                    <condition>true</condition>
                  </read-only>
                </properties>
                <directories>
                  <directory-ref ref="ndoStatusAppre" />
                </directories>
                <appearance align="right">
                  <label-style field-width="16" field-unit="symbol">
                    <style id="tunBold"/>
                  </label-style>
                </appearance>
              </field>
              <field attribute-ref="statusExtended">
                <properties>
                  <read-only>
                    <condition>true</condition>
                  </read-only>
                </properties>
                <directories>
                  <directory-ref ref="ndoStatusExtended" />
                </directories>
                <appearance align="right">
                  <label-style field-width="16" field-unit="symbol">
                    <style id="tunBold"/>
                  </label-style>
                </appearance>
              </field>
            </component>
          </section>
          <section id="Header_row3" group="true">
            <appearance field-layout="horizontal"/>
            <component>
              <button name="${ui.form.NdoRegister.label.approvedByStateArch.name:Согласовано госархивом}" tid="approvedByStateArch">
                <properties>
                  <hide>
                    <negative-condition-ref ref="NdoIsApprovedByStateArch"/>
                  </hide>
                  <disabled condition-union="AND">
                    <negative-condition-ref ref="read"/>
                    <negative-condition-ref ref="edit"/>
                  </disabled>
                </properties>
                <appearance>
                  <label-style>
                    <style id="tunBtnHeader" />
                    <style id="tunGreen" />
                  </label-style>
                </appearance>
              </button>
              <button recalc="true" attribute-ref="isArchInstEpkApproved" tid="unApprovedByStateArch"
                      name="${ui.form.NdoRegister.label.unApprovedByStateArch.name:Не согласовано госархивом}">
                <properties>
                  <hide>
                    <condition-ref ref="NdoIsApprovedByStateArch"/>
                  </hide>
                  <disabled>
                    <negative-condition-ref ref="NdoCanBeApprovedByStateArch"/>
                  </disabled>
                </properties>
                <appearance>
                  <label-style>
                    <style id="tunBtnHeader" />
                    <style id="tunRed" />
                  </label-style>
                </appearance>
              </button>
              <field attribute-ref="archInstProtocol" name="${ui.form.NdoRegisterForm.label.archInstProtocol.name:Протокол ЭПК архивного учреждения от}">
                <properties>
                  <require>
                    <condition>true</condition>
                  </require>
                  <hide>
                    <negative-condition-ref ref="NdoIsApprovedByStateArch"/>
                  </hide>
                </properties>
                <appearance align="left">
                  <label-style label-width="20" label-unit="symbol" field-width="16" field-unit="symbol"/>
                </appearance>
              </field>
              <field attribute-ref="archInstNum" name="${ui.form.NdoRegisterForm.label.archInstNum.name:№}">
                <properties>
                  <require>
                    <condition>true</condition>
                  </require>
                  <hide>
                    <negative-condition-ref ref="NdoIsApprovedByStateArch"/>
                  </hide>
                </properties>
                <appearance align="left">
                  <label-style label-width="3" label-unit="symbol" field-width="40" field-unit="symbol"/>
                </appearance>
              </field>
            </component>
          </section>
        </component>
      </section>
      <section id="ContentSection" name="${ui.form.FinanceResourceForm.section.ContentSection.name:Содержание}">
        <appearance widget="leftMenu"/>
        <properties>
          <hide condition-union="AND">
            <condition-ref ref="ndoContentEmpty"/>
          </hide>
        </properties>
        <component>
          <field attribute-ref="contentFiles" name="${ui.form.NdoRegisterForm.content.section.contentFiles.name:Файлы}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="contentFilesNotEmpty"/>
              </hide>
            </properties>
            <appearance importance="normal"/>
          </field>
          <field attribute-ref="contentFiles" name="${ui.form.NdoRegisterForm.content.section.contentFiles.name:Файлы}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="contentFilesEmpty"/>
                <negative-condition-ref ref="edit"/>
              </hide>
            </properties>
            <appearance importance="normal"/>
          </field>
          <field attribute-ref="contentImage" name="${ui.form.NdoRegisterForm.content.section.Image.contentImage.name:Образ после подписания}">
            <properties>
              <read-only>
                <negative-condition-ref ref="canEditImage"/>
              </read-only>
              <hide condition-union="AND">
                <negative-condition-ref ref="contentImageNotEmpty"/>
              </hide>
            </properties>
            <appearance importance="normal"/>
          </field>
          <field attribute-ref="contentImage" name="${ui.form.NdoRegisterForm.content.section.Image.contentImage.name:Образ после подписания}">
            <properties>
              <read-only>
                <negative-condition-ref ref="canEditImage"/>
              </read-only>
              <hide condition-union="AND">
                <negative-condition-ref ref="contentImageEmpty"/>
                <negative-condition-ref ref="edit"/>
              </hide>
            </properties>
            <appearance importance="normal"/>
          </field>
        </component>
      </section>
      <section id="DealsSection" name="${ui.form.NdoRegisterForm.section.DealsSection.name:Дела}">
        <properties>
          <read-only>
            <condition-ref ref="exStatusIsEkApproved"/>
          </read-only>
        </properties>
        <component>
          <section group="true" id="PartiotionsRow">
            <component>
              <section group="true" id="RowButtons">
                <component>
                  <button name="${ui.form.NdoRegisterForm.section.DealsSection.section.PartiotionsRow.button.CreateNew.name:Создать новый раздел}">
                    <properties>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="canAddOrCreateChapterNdo"/>
                      </hide>
                    </properties>
                    <appearance align="left">
                      <label-style width="20">
                        <style id="tunUnderline" />
                        <style id="tunClickableRed" />
                      </label-style>
                    </appearance>
                    <action>
                      <custom>
                        <operation>
                          <handler>
                            <script lang="JavaBean">CreateNewNdpForNdo</script>
                          </handler>
                        </operation>
                      </custom>
                    </action>
                  </button>
                  <button name="${ui.form.NdoRegisterForm.section.DealsSection.section.PartiotionsRow.button.LoadFromXlsx.name:Загрузить данные из .xlsx}">
                    <properties>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="canAddOrCreateChapterNdo"/>
                      </hide>
                    </properties>
                    <appearance align="left">
                      <label-style width="20">
                        <style id="tunUnderline" />
                        <style id="tunClickableRed" />
                      </label-style>
                    </appearance>
                    <action>
                      <custom>
                        <operation>
                          <handler>
                            <script lang="JavaBean">NdoLoadFromXlsxOperation</script>
                          </handler>
                        </operation>
                      </custom>
                    </action>
                  </button>
                  <button name="${ui.form.NdoRegisterForm.section.DealsSection.section.PartiotionsRow.button.UnloadToXlsx.name:Выгрузить данные в .xlsx}">
                    <properties>
                      <hide condition-union="AND">
                        <negative-condition-ref ref="canUnloadChapterNdo"/>
                      </hide>
                    </properties>
                    <appearance align="left">
                      <label-style width="20">
                        <style id="tunUnderline" />
                        <style id="tunClickableRed" />

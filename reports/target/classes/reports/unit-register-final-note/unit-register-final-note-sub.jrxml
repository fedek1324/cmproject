	<queryString>
		<![CDATA[SELECT
	0 AS ord,
	'Постоянного' AS columnName,
	sumTotalSpd AS total,
	movable,
	epkMark
FROM FR_UnitRegister
WHERE id = $P{ndp}

UNION ALL

SELECT
	1,
	'Временного<br>(свыше 10 лет)',
	gt10_SumTemporalOver,
	gt10_Movable,
	gt10_EpkMark
FROM FR_UnitRegister
WHERE id = $P{ndp}

UNION ALL

SELECT
	2,
	'Временного<br>(до 10 лет)',
	lt10_SumTemporalLess,
	lt10_Movable,
	lt10_EpkMark
FROM FR_UnitRegister
WHERE id = $P{ndp}

UNION ALL

SELECT
	3,
	'Итого',
	sum AS total,
	sum_Movable,
	sum_EpkMark
FROM FR_UnitRegister
WHERE id = $P{ndp}]]>
	</queryString>
